{
  "expo": {
    "name": "PySer Tickets",
    "slug": "pyser-tickets",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [ "**/*" ],
    "android": {
      "package": "com.pyser.tickets"
    },
    "ios": {
      "bundleIdentifier": "com.pyser.tickets",
      "supportsTablet": true
    }
  }
}